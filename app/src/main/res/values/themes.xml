<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.TimetableISTU" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/dark_black_transparent</item>
        <item name="colorPrimaryVariant">@color/black_transparent</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.TimetableISTU.Launcher">
        <item name="android:windowBackground">@drawable/launch_screen</item>
    </style>

    <style name="ProgressBarStyle">
        <item name="colorAccent">@color/dirty_yellow</item>
    </style>
</resources>
